import type { <%=typeName%>, IPageSearch } from "@/types";

const searchConfig: IPageSearch<<%=typeName%>> = {
  labelWidth: "100px",
  formItems: [
  <% fileds.forEach(function (filed, index) { %> 
    {
      type: "<%=filed.type%>",
      span: 6,
      formItemBind: {
        prop: "<%=filed.prop%>",
        label: "<%=filed.label%>",
      },
      componentBind: {
        clearable: true,
        placeholder: "请输入<%=filed.label%>",
      }
    }<% if (index < fileds.length - 1) { %>,<% } %>
  <% }); %>
  ],
};

export default searchConfig;
